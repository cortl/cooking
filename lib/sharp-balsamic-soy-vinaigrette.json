  "ingredients": [
    {
      "category": "All",
      "items": [
        "3 tablespoons balsamic vinegar",
        "1 tablespoon soy sauce",
        "4 teaspoons dijon mustard",
        "1 small shallot (minced or grated on a Microplane)",
        "1 medium garlic clove",
        "1/2 cup canola oil",
        "1/4 cup extra virgin olive oil",
        "1/2 teaspoon salt",
        "1/4 teaspoon ground black pepper"
      ]
    }
  ],
  "instructions": [
    "Combine all ingredients in a small container or squeeze bottle. Seal the container and shake vigorously until emulsified. The vinaigrette will keep in the refrigerator for up to 3 months; shake vigorously before using."
  ],
